{
  "name": "@star__hoshi/orderable",
  "version": "0.3.0",
  "description": "",
  "main": "out/orderable.js",
  "types": "out/orderable.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tslint -p tslint.json && tsc -d",
    "publish:patch": "yarn run build && npm version patch && git commit -am 'publish' && git push origin master && npm publish --access=public",
    "publish:minor": "yarn run build && npm version minor && git commit -am 'publish' && git push origin master && npm publish --access=public",
    "publish:major": "yarn run build && npm version major && git commit -am 'publish' && git push origin master && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starhoshi/orderable.ts.git"
  },
  "keywords": [
    "firebase",
    "stripe"
  ],
  "author": "star__hoshi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/starhoshi/orderable.ts/issues"
  },
  "homepage": "https://github.com/starhoshi/orderable.ts#readme",
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/request": "^2.47.0",
    "@types/slack-node": "^0.1.2",
    "@types/stripe": "^5.0.4",
    "firebase-tools": "^3.17.4",
    "jest": "^22.1.4",
    "ts-jest": "^22.0.3",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "jest": {
    "transform": {
      "\\.ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/__tests__/.*.test.ts",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "@1amageek/flow": "^0.2.1",
    "firebase-admin": "^5.8.2",
    "firebase-functions": "^0.8.1",
    "pring": "^0.5.2",
    "request": "^2.83.0",
    "retrycf": "^0.1.1",
    "slack-node": "^0.1.8",
    "stripe": "^5.4.0"
  }
}
